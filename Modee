require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game).caption("Helloooooooo!",true)
wait(2) require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game).caption("Very Good luck!",true)

--Cave ambience
local sound = Instance.new("Sound")

sound.SoundId = "rbxassetid://8734471313"

sound.Volume = 3

sound.Looped = true

sound:Play()

sound.Parent = workspace

-- new fog
local Lighting = game:GetService("Lighting")
Lighting.Ambient = Color3.new(0, 0, 0)
Lighting.Brightness = 0.1
Lighting.FogEnd = 50
Lighting.FogStart = 35
Lighting.FogColor = Color3.new(0, 0, 0)

